<%=
  raw(CSV.generate do |csv|
    csv << [User.human_attribute_name(:email), t('users.last_sign_in_at'), t('users.has_news_letter_subscription'), t('users.type')]
    @users.each do |user|
      csv << [user.email_address, last_sign_in_at(user), has_news_letter_subscription(user), user_type(user)]
    end
  end)
%>
